{
    "version": 2,
    "builds": [
      { 
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { 
        "src": "/api/generate-pdf",
        "methods": ["POST"],
        "dest": "index.js"
      }
    ],
    "functions": {
      "index.js": {
        "memory": 1024,
        "maxDuration": 30
      }
    }
}